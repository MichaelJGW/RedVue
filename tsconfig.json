{
    "compilerOptions": {
        "module": "commonjs",
        "target":"es5",
        "outDir": "./dist/",
        "declaration": true,
        "sourceMap": true, 
        "noImplicitAny": false,
        "removeComments": true,
        "preserveConstEnums": true,
        "baseUrl": "./dist",
        "lib": [ "esnext", "es5" ]
    },
    "included": [
        "src/**/*"
    ],
    "exclude": [
        "node_modules",
        "examples"
    ]
}